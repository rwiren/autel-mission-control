        }
      },
      "pluginVersion": "12.3.0",
      "targets": [
        {
          "query": "from(bucket: \"telemetry\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"mqtt_consumer\")\n  |> filter(fn: (r) => r[\"_field\"] == \"data_latitude\" or r[\"_field\"] == \"data_longitude\")\n  \n  // FIX 1: REMOVED the empty \"|> group()\" command here. \n  // We want to keep the data separated by its original tags (drone vs controller)\n  \n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  \n  // Filter out invalid GPS data\n  |> filter(fn: (r) => exists r[\"data_latitude\"] and exists r[\"data_longitude\"])\n  |> filter(fn: (r) => r[\"data_latitude\"] != 0.0)\n  \n  |> map(fn: (r) => ({\n      _time: r._time,\n      lat: r.data_latitude,\n      lon: r.data_longitude,\n      // FIX 2: Pass the distinguishing tag (topic) to Grafana\n      source: r.topic\n  }))\n  \n  // FIX 3: Group by this tag so Grafana draws two separate lines\n  |> group(columns: [\"source\"])\n  |> sort(columns: [\"_time\"])",
          "refId": "A",
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB_TELEMETRY}"
          }
        }
      ],
