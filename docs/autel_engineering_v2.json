        {
          "query": "import \"strings\"\n\nfrom(bucket: \"telemetry\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"mqtt_consumer\")\n  // 1. Target the percentage field (common to both devices)\n  |> filter(fn: (r) => r[\"_field\"] == \"data_capacity_percent\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  \n  // 2. LOGIC: Check the Topic to see who is talking\n  |> map(fn: (r) => ({\n      _time: r._time,\n      _value: r._value,\n      _field: if strings.containsStr(v: r.topic, substr: \"TH\") then \"Controller Battery\" else \"Drone Battery\"\n  }))\n  \n  // 3. CLEAN: Keep only what we need to force a clean Legend\n  |> keep(columns: [\"_time\", \"_value\", \"_field\"])",
          "refId": "A"
        }
      ],
      "title": "Controller battery",
      "type": "timeseries"
